<div class="poll-result"></div>
<div class="poll-info well span12">
    <h1><%= poll_title %></h1>
    <% if (rep_title) { %>
    <h2>Question from: <%= rep_title %></h2> 
    <% } %>
    <p><%= prompt %></p>
    <% _.each(options, function(key,num) { %>
    <button class="pollAnswer btn btn-large" data-pollid="<%= poll_id %>" 
                               data-answerid="<%= num %>">
        <%= key %>
    </button>
    <% }); %>
    <br>
    <a href="#" class="pollSkip btn btn-primary" data-pollid="<%= poll_id %>" 
                               data-answerid="99">
        Skip
    </a>
    <br>
    <br>
</div>
        

