<div class="poll-info well span8">
    <p><%= prompt %></p>
    <% _.each(options, function(key,num) { %>
    <button class="pollAnswer btn btn-large" data-pollid="<%= poll_id %>" 
                               data-answerid="<%= num %>">
        <%= key %>
    </button>
    <% }); %>
    <br>
    <a href="#/<%= poll_id %>/answer" class="pollView btn btn-danger btn-large" data-pollid="<%= poll_id %>" 
                               data-answerid="99">
        Prompt Question
    </a>
    <button class="pollNext btn btn-large" data-pollid="<%= poll_id %>" 
                               data-answerid="99">
        Skip
    </button>
    <br>
    <br>
</div>
        

